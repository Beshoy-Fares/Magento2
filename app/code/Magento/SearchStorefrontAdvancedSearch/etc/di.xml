<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\SearchStorefrontAdvancedSearch\Model\Client\ClientFactoryInterface" type="Magento\SearchStorefrontAdvancedSearch\Model\Client\ClientFactory" />
    <type name="Magento\SearchStorefrontAdvancedSearch\Block\Recommendations">
        <arguments>
            <argument name="searchDataProvider" xsi:type="object">Magento\SearchStorefrontAdvancedSearch\Model\Recommendations\DataProvider</argument>
            <argument name="title" xsi:type="string" translatable="true">Related search terms</argument>
        </arguments>
    </type>
    <type name="Magento\SearchStorefrontAdvancedSearch\Block\Suggestions">
        <arguments>
            <argument name="searchDataProvider" xsi:type="object">Magento\SearchStorefrontAdvancedSearch\Model\SuggestedQueries</argument>
            <argument name="title" xsi:type="string" translatable="true">Did you mean</argument>
        </arguments>
    </type>
    <type name="Magento\SearchStorefrontAdvancedSearch\Model\SuggestedQueries">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="mysql" xsi:type="string">Magento\SearchStorefrontAdvancedSearch\Model\DataProvider\Suggestions</item>
            </argument>
        </arguments>
    </type>
    <preference for="Magento\SearchStorefrontAdvancedSearch\Model\Adapter\DataMapper\AdditionalFieldsProviderInterface" type="Magento\SearchStorefrontAdvancedSearch\Model\Adapter\DataMapper\AdditionalFieldsProvider" />
</config>
