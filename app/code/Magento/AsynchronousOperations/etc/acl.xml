<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Magento_AsynchronousOperations::magento_logging" title="Action Log" translate="title" sortOrder="70">
                        <resource id="Magento_AsynchronousOperations::magento_logging_events" title="Report" translate="title" sortOrder="10"/>
                        <resource id="Magento_AsynchronousOperations::system_magento_logging_bulk_operations" title="Bulk Actions / Own Bulk Actions" translate="title" sortOrder="15">
                            <resource id="Magento_AsynchronousOperations::system_magento_logging_bulk_operations_guests" title="Guest Bulk Actions" translate="title" sortOrder="15"/>
                            <resource id="Magento_AsynchronousOperations::system_magento_logging_bulk_operations_customers" title="Customer Bulk Actions" translate="title" sortOrder="30"/>
                            <resource id="Magento_AsynchronousOperations::system_magento_logging_bulk_operations_integrations" title="Global Integration Bulk Actions" translate="title" sortOrder="15"/>
                            <resource id="Magento_AsynchronousOperations::system_magento_logging_bulk_operations_admin" title="Global Admin Bulk Actions" translate="title" sortOrder="15"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
